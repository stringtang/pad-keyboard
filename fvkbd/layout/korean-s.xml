<!-- This layout is for Korean with Dubeolsik layout -->

<keyboard num_of_modes="3" font="Droid Sans_bold_16" efont="Droid Sans_bold_12" ofont="Droid Sans_bold_24"
panel_bgcolor="6a6a6a" key_bgcolor="303030" key_fgcolor="fcc536"
pop_bgcolor="fcc536" pop_fgcolor="303030" key_extra_fgcolor="838383">

	<mode id="0" name="default"/>
	<mode id="1" name="CapsLock"/>
	<mode id="2" name="symbol"/>

	<panel layout="xy" x="0" y="0" width="800" height="196" key_shape="chamfer" dock="none">
		<key x="4" y="5" width="55" height="58">
			<action disp="↑" type="func">mode:1</action>
			<action mode_id="1" disp="↑" type="func">mode:0</action>
			<action mode_id="2" disp="↑" type="func">mode:0</action>
		</key>

		<key x="65" y="5" width="55" height="58">
			<action disp="q" type="strings">q ㅂ</action>
			<action mode_id="1" disp="Q" type="strings">Q ㅃ</action>
			<action mode_id="2" disp="1">1</action>
		</key>

		<key x="126" y="5" width="55" height="58">
			<action disp="w" type="strings">w ㅈ</action>
			<action mode_id="1" disp="W" type="strings">W ㅉ</action>
			<action mode_id="2" disp="2">2</action>
		</key>

		<key x="187" y="5" width="55" height="58">
			<action disp="e" type="strings">e ㄷ é è ê ë</action>
			<action mode_id="1" disp="E" type="strings">E ㄸ É È Ê Ë</action>
			<action mode_id="2" disp="3">3</action>
		</key>

		<key x="248" y="5" width="55" height="58">
			<action disp="r" type="strings">r ㄱ</action>
			<action mode_id="1" disp="R" type="strings">R ㄲ</action>
			<action mode_id="2" disp="4">4</action>
		</key>

		<key x="309" y="5" width="55" height="58">
			<action disp="t" type="strings">t ㅅ</action>
			<action mode_id="1" disp="T" type="strings">T ㅆ</action>
			<action mode_id="2" disp="5">5</action>
		</key>

		<key x="370" y="5" width="55" height="58">
			<action disp="y" type="strings">y ㅛ ÿ</action>
			<action mode_id="1" disp="Y" type="strings">Y ㅛ Ÿ</action>
			<action mode_id="2" disp="6">6</action>
		</key>

		<key x="431" y="5" width="55" height="58">
			<action disp="u" type="strings">u ㅕ û ù ü ú</action>
			<action mode_id="1" disp="U" type="strings">U ㅕ Û Ù Ü Ú</action>
			<action mode_id="2" disp="7">7</action>
		</key>

		<key x="492" y="5" width="55" height="58">
			<action disp="i" type="strings">i ㅑ î ï í</action>
			<action mode_id="1" disp="I" type="strings">I ㅑ Î Ï Í</action>
			<action mode_id="2" disp="8">8</action>
		</key>

		<key x="553" y="5" width="55" height="58">
			<action disp="o" type="strings">o ㅐ ô ö ó õ</action>
			<action mode_id="1" disp="O" type="strings">O ㅒ Ô Ö Ó Õ</action>
			<action mode_id="2" disp="9">9</action>
		</key>

		<key x="614" y="5" width="55" height="58">
			<action disp="p" type="strings">p ㅔ</action>
			<action mode_id="1" disp="P" type="strings">P ㅖ</action>
			<action mode_id="2" disp="0">0</action>
		</key>

		<key x="675" y="5" width="55" height="58">
		<action disp="←" img="key_bs.png" img_dn="key_bs_dn.png" type="sym">BackSpace</action>
		</key>

		<key x="736" y="5" width="60" height="58">
			<action img="key_set.png" img_dn="key_set_dn.png" disp="IM" type="script">menu</action>
		</key>

		<!--next row-->

		<key x="4" y="69" width="55" height="58">
			<action disp="123" type="func">mode:2</action>
			<action mode_id="2" disp="123" type="func">mode:0</action>
		</key>

		<key x="65" y="69" width="55" height="58">
			<action disp="a" type="strings">a ㅁ à â ä ã á å</action>
			<action mode_id="1" disp="A" type="strings">A ㅁ À Â Ä Ã Á Å</action>
			<action mode_id="2" disp="-">-</action>
		</key>

		<key x="126" y="69" width="55" height="58">
			<action disp="s" type="strings">s ㄴ ß</action>
			<action mode_id="1" disp="S" type="strings">S ㄴ ß</action>
			<action mode_id="2" disp="/">/</action>
		</key>

		<key x="187" y="69" width="55" height="58">
			<action disp="d" type="strings">d ㅇ</action>
			<action mode_id="1" disp="D" type="strings">D ㅇ</action>
			<action mode_id="2" disp=":">:</action>
		</key>

		<key x="248" y="69" width="55" height="58">
			<action disp="f" type="strings">f ㄹ</action>
			<action mode_id="1" disp="F" type="strings">F ㄹ</action>
			<action mode_id="2" disp=";">;</action>
		</key>

		<key x="309" y="69" width="55" height="58">
			<action disp="g" type="strings">g ㅎ</action>
			<action mode_id="1" disp="G" type="strings">G ㅎ</action>
			<action mode_id="2" disp="(" type="strings">( [ { 「 《 ﹤</action>
		</key>

		<key x="370" y="69" width="55" height="58">
			<action disp="h" type="strings">h ㅗ</action>
			<action mode_id="1" disp="H" type="strings">H ㅙ</action>
			<action mode_id="2" disp=")" type="strings">) ] } 」 》 ﹥</action>
		</key>

		<key x="431" y="69" width="55" height="58">
			<action disp="j" type="strings">j ㅓ</action>
			<action mode_id="1" disp="J" type="strings">J ㅝ</action>
			<action mode_id="2" disp="£" type="strings">£ $ € ¥</action>
		</key>

		<key x="492" y="69" width="55" height="58">
			<action disp="k" type="strings">k ㅏ</action>
			<action mode_id="1" disp="K" type="strings">K ㅘ</action>
			<action mode_id="2" disp="&amp;">&amp;</action>
		</key>

		<key x="553" y="69" width="55" height="58">
			<action disp="l" type="strings">l ㅣ</action>
			<action mode_id="1" disp="L" type="strings">L ㅚ</action>
			<action mode_id="2" disp="#">#</action>
		</key>

		<key x="614" y="69" width="55" height="58">
			<action disp=".">.</action>
			<action mode_id="2" disp="$">$</action>
		</key>

		<key x="675" y="69" width="55" height="58">
			<action disp="?">?</action>
			<action mode_id="2" disp="!">!</action>
		</key>

		<key x="736" y="69" width="60" height="122" pop="none">
			<action img="key_return.png" img_dn="key_return_dn.png" disp="◀┛" type="sym">Return</action>
		</key>

		<!--next row-->

		<key x="4" y="133" width="55" height="58">
			<action img="key_hide.png" img_dn="key_hide_dn.png" disp="↧" type="func">exit</action>
		</key>

		<key x="65" y="133" width="55" height="58">
			<action disp="z" type="strings">z ㅋ</action>
			<action mode_id="1" disp="Z" type="strings">Z ㅋ</action>
			<action mode_id="2" disp='"' type="strings">" “ ” ‘ ’</action>
		</key>

		<key x="126" y="133" width="55" height="58">
			<action disp="x" type="strings">x ㅌ</action>
			<action mode_id="1" disp="X" type="strings">X ㅌ</action>
			<action mode_id="2" disp="%">%</action>
		</key>

		<key x="187" y="133" width="55" height="58">
			<action disp="c" type="strings">c ㅊ ç</action>
			<action mode_id="1" disp="C" type="strings">C ㅊ Ç</action>
			<action mode_id="2" disp="&lt;">&lt;</action>
		</key>

		<key x="248" y="133" width="55" height="58">
			<action disp="v" type="strings">v ㅍ</action>
			<action mode_id="1" disp="V" type="strings">V ㅍ</action>
			<action mode_id="2" disp="&gt;">&gt;</action>
		</key>

		<key x="309" y="133" width="55" height="58">
			<action disp="b" type="strings">b ㅠ</action>
			<action mode_id="1" disp="B" type="strings">B ㅟ</action>
			<action mode_id="2" disp="_">_</action>
		</key>

		<key x="370" y="133" width="55" height="58">
			<action disp="n" type="strings">n ㅜ</action>
			<action mode_id="1" disp="N" type="strings">N ㅞ</action>
			<action mode_id="2" disp="+">+</action>
		</key>

		<key x="431" y="133" width="55" height="58">
			<action disp="m" type="strings">m ㅡ</action>
			<action mode_id="1" disp="M" type="strings">M ㅢ</action>
			<action mode_id="2" disp=".com" type="strings" font="Droid Sans_bold_14" ofont="Droid Sans_bold_14">.com .net .org .edu</action>
		</key>

		<key x="492" y="133" width="175" height="58" pop="none">
			<action disp=""> </action>
		</key>

		<key x="675" y="133" width="55" height="58">
			<action disp="@">@</action>
		</key>


	</panel>

</keyboard>
