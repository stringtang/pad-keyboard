<keyboard num_of_modes="4" font="Droid Sans_bold_18" efont="Droid Sans_bold_12" ofont="Droid Sans_bold_26"
panel_bgcolor="2e2e2e" key_bgcolor="2e2e2e" key_fgcolor="2e2e2e"
pop_bgcolor="fcc536" pop_fgcolor="303030" key_extra_fgcolor="838383">

	<mode id="0" name="default"/>
	<mode id="1" name="CapsLock"/>
	<mode id="2" name="symbol"/>
	<mode id="3" name="test"/>

	<panel layout="xy" x="0" y="0" width="800" height="265" key_shape="chamfer" dock="bottom"> 
	    <!--first line-->

		<key x="6" y="5" width="78" height="58">
		    <action  img="normal-hide-up.png" img_dn="press-hide-up.png"   type="script">move-keyboard.py</action>  
		</key>

